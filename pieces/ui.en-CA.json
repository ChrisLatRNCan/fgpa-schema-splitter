{
  "description": "A set of service endpoints used by the viewer",
  "type": "object",
  "properties": {
    "fullscreen": {
      "type": "boolean",
      "default": false,
      "description": "Indicates viewer takes up entire viewport",
      "title": "[en]"
    },
    "theme": {
      "type": "string",
      "enum": [
        "default"
      ],
      "default": "default",
      "description": "UI theme of the viewer",
      "title": "[en]"
    },
    "logoUrl": {
      "type": "string",
      "description": "An optional image to be used in the place of the default viewer logo",
      "title": "[en]"
    },
    "failureFeedback": {
      "failureMessage": {
        "type": "string",
        "description": "An optional message to be used in place of the failure message"
      },
      "failureImageUrl": {
        "type": "string",
        "description": "An optional image to be used in place of the failure Image"
      },
      "title": "[en]"
    },
    "title": {
      "type": "string",
      "description": "An optional title to be used in the place of the default viewer title",
      "title": "[en]"
    },
    "appBar": {
      "default": {
        "sideMenu": true,
        "geoSearch": true,
        "layers": true
      },
      "description": "Provides configuration to the main app toolbar. If not supplied, the all default appbar controls are displayed. To completely hide the toolbar, provide the following: { sideMenu: false, geoSearch: false, layers: false }.",
      "type": "object",
      "properties": {
        "sideMenu": {
          "type": "boolean",
          "default": true,
          "description": "appBarNode.sideMenu.description",
          "title": "[en]"
        },
        "geoSearch": {
          "type": "boolean",
          "default": true,
          "description": "appBarNode.geoSearch.description",
          "title": "[en]"
        },
        "basemap": {
          "type": "boolean",
          "default": true,
          "description": "appBarNode.basemap.description",
          "title": "[en]"
        },
        "layers": {
          "type": "boolean",
          "default": true,
          "description": "appBarNode.layers.description",
          "title": "[en]"
        }
      },
      "additionalProperties": false,
      "schema": "appBarNode",
      "title": "[en]"
    },
    "navBar": {
      "type": "object",
      "properties": {
        "zoom": {
          "type": "string",
          "enum": [
            "navBarNode.zoom.enum.all",
            "navBarNode.zoom.enum.buttons",
            "navBarNode.zoom.enum.slider"
          ],
          "default": "buttons",
          "title": "[en]"
        },
        "extra": {
          "default": [
            "fullscreen",
            "geoLocator",
            "home",
            "help"
          ],
          "title": "[en]",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "navBarButtons.items.enum.geoLocator",
              "navBarButtons.items.enum.marquee",
              "navBarButtons.items.enum.home",
              "navBarButtons.items.enum.history",
              "navBarButtons.items.enum.basemap",
              "navBarButtons.items.enum.help",
              "navBarButtons.items.enum.fullscreen",
              "navBarButtons.items.enum.geoSearch",
              "navBarButtons.items.enum.sideMenu",
              "navBarButtons.items.enum.layers"
            ]
          },
          "schema": "navBarButtons"
        }
      },
      "required": [
        "zoom"
      ],
      "additionalProperties": false,
      "schema": "navBarNode",
      "title": "[en]"
    },
    "sideMenu": {
      "type": "object",
      "properties": {
        "logo": {
          "type": "boolean",
          "default": true,
          "description": "sideMenuNode.logo.description",
          "title": "[en]"
        },
        "items": {
          "type": "array",
          "items": {
            "type": "array",
            "uniqueItems": true,
            "items": {
              "type": "string",
              "enum": [
                "layers",
                "basemap",
                "geoSearch",
                "about",
                "fullscreen",
                "export",
                "share",
                "touch",
                "help",
                "language",
                "plugins"
              ]
            },
            "schema": "sideMenuButtons",
            "title": "[en]"
          },
          "default": [
            [
              "layers",
              "basemap"
            ],
            [
              "fullscreen",
              "export",
              "share",
              "touch",
              "help",
              "about"
            ],
            [
              "language"
            ],
            [
              "plugins"
            ]
          ],
          "title": "[en]"
        }
      },
      "additionalProperties": false,
      "description": "sideMenuNode.description",
      "schema": "sideMenuNode",
      "title": "[en]"
    },
    "restrictNavigation": {
      "type": "boolean",
      "default": false,
      "description": "Will restrict the user from panning beyond the maximum extent.",
      "title": "[en]"
    },
    "about": {
      "description": "About properties from configuration file or Markdown folder",
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "content": {
              "type": "string"
            }
          },
          "required": [
            "content"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "folderName": {
              "type": "string"
            }
          },
          "required": [
            "folderName"
          ],
          "additionalProperties": false
        }
      ],
      "title": "[en]"
    },
    "help": {
      "type": "object",
      "description": "Help properties",
      "properties": {
        "folderName": {
          "type": "string",
          "description": "Help folder name who contain the help description and images",
          "title": "[en]"
        }
      },
      "required": [
        "folderName"
      ],
      "additionalProperties": false,
      "title": "[en]"
    },
    "legend": {
      "type": "object",
      "properties": {
        "reorderable": {
          "type": "boolean",
          "default": true,
          "description": "Specifies if the items in the legend can be reordered; structured legend ignores this property.",
          "title": "[en]"
        },
        "allowImport": {
          "type": "boolean",
          "default": true,
          "description": "Specifies if the user-added layers are allowed.",
          "title": "[en]"
        },
        "isOpen": {
          "type": "object",
          "description": "Specifies whether the legend is opened by default on initial loading of the map for small, medium, and large viewports",
          "properties": {
            "large": {
              "type": "boolean",
              "default": false,
              "description": "Whether the legend is opened by default on initial loading of the map for large viewports",
              "title": "[en]"
            },
            "medium": {
              "type": "boolean",
              "default": false,
              "description": "Whether the legend is opened by default on initial loading of the map for medium viewports",
              "title": "[en]"
            },
            "small": {
              "type": "boolean",
              "default": false,
              "description": "Whether the legend is opened by default on initial loading of the map for small viewports",
              "title": "[en]"
            }
          },
          "additionalProperties": false,
          "title": "[en]"
        }
      },
      "additionalProperties": false,
      "title": "[en]"
    },
    "tableIsOpen": {
      "type": "object",
      "description": "Specifies whether the table panel is opened by default on initial loading of the map for small, medium, and large viewports",
      "properties": {
        "id": {
          "type": "string",
          "description": "The id of the layer for referencing within the viewer",
          "title": "[en]"
        },
        "large": {
          "type": "boolean",
          "default": false,
          "description": "Whether the table panel is opened by default on initial loading of the map for large viewports",
          "title": "[en]"
        },
        "medium": {
          "type": "boolean",
          "default": false,
          "description": "Whether the table panel is opened by default on initial loading of the map for medium viewports",
          "title": "[en]"
        },
        "small": {
          "type": "boolean",
          "default": false,
          "description": "Whether the table panel is opened by default on initial loading of the map for small viewports",
          "title": "[en]"
        }
      },
      "additionalProperties": false,
      "required": [
        "id"
      ],
      "title": "[en]"
    },
    "widgetsWidget": {
      "type": "object",
      "additionalProperties": true,
      "description": "FIXME",
      "title": "[en]"
    }
  },
  "schema": "ui",
  "title": "[en]"
}